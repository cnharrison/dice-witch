{
  "extends": [
    "airbnb-base",
    "eslint:recommended",
    "prettier",
    "plugin:security/recommended"
  ],
  "parserOptions": {
    "sourceType": "module",
    "ecmaVersion": 2020
},
"env": {
  "es6": true,
  "node": true
},
"plugins": [
  "prettier"
],
"rules": {
  "no-useless-escape": "off",
  "max-len": "off",
  "indent": "off",
  "prettier/prettier": "error",
  "new-cap": "off",
  "global-require": "off"
}

}
